{
  "name": "crawl-e",
  "version": "0.5.5",
  "description": "Showtimes Crawler Framework",
  "author": "Cinepass (Martin Stemmle)",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "private": true,
  "scripts": {
    "docs": "docsify serve docs -p 9999",
    "typedoc": "typedoc",
    "test": "ts-mocha -p tsconfig.json --reporter spec --timeout 5000 '**/*.*test.ts'",
    "ts-mocha": "ts-mocha",
    "build": "tsc",
    "browserify-utils": "browserify lib/Utils.exports-browser.js -o docs/_js/crawl-e.utils.bundle.js",
    "generate:schemas": "yarn run generate:schemas:cinema && yarn run generate:schemas:movie && yarn run generate:schemas:version",
    "generate:schemas:cinema": "typescript-json-schema src/models/Cinema.ts Cinema --required -o spec/cinema-schema.json",
    "generate:schemas:movie": "typescript-json-schema src/models/Movie.ts Movie --required -o spec/movie-schema.json",
    "generate:schemas:version": "typescript-json-schema src/models/MovieVersion.ts MovieVersion --required -o spec/version-schema.json",
    "generate:schemas:dates-parsing-config": "typescript-json-schema src/parsers/DatesParsing.ts DatesParsing.Config --required -o spec/dates-parsing-config.json",
    "generate:schemas:date-page": "typescript-json-schema src/ResponseParsers.ts DatePage --required -o spec/datepage-schema.json",
    "generate:schemas:table-cell-reference": "typescript-json-schema src/parsers/TableParser.ts TableParser.CellReference --required -o spec/table-cell-reference-schema.json",
    "generate:schemas:context": "typescript-json-schema src/Context.ts Context -o spec/context-schema.json",
    "generate:hooks-schema": "typescript-json-schema src/Config.ts SubConfigs.Hooks -o spec/config-schema_hooks.json && node spec/_fix_hook_types.js"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^7.0.2",
    "@types/underscore": "^1.8.6",
    "chai": "^4 .1.2",
    "deepcopy": "^2.0.0",
    "docsify": "^4.11.3",
    "docsify-cli": "^4.4.0",
    "mocha": "^7.2.0",
    "pretty": "^2.0.0",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.0.3",
    "ts-transformer-keys": "^0.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^8.0.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typedoc": "^0.17.7",
    "typescript": "^3.3",
    "typescript-json-schema": "^0.42.0"
  },
  "dependencies": {
    "@types/async": "^2.0.45",
    "@types/cheerio": "^0.22.6",
    "@types/superagent": "^4.1",
    "ajv": "^6.10.0",
    "ascii-table": "^0.0.9",
    "async": "^2.6.0",
    "browserify": "^16.2.3",
    "cheerio": "^1.0.0-rc.2",
    "cheerio-tableparser": "git+https://github.com/CINEPASS/cheerio-tableparser.git#master",
    "colors": "^1.1.2",
    "commander": "^5.1.0",
    "debug": "^4.1.0",
    "iso-639-1": "^2.0.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "nock": "^10.0.6",
    "object-path": "^0.11.4",
    "path": "^0.12.7",
    "random-useragent": "^0.3.1",
    "superagent": "^4.1",
    "superagent-charset": "^1.2.0",
    "superagent-proxy": "^2.0.0",
    "tv4": "^1.3.0",
    "underscore": "^1.8.3"
  }
}
